{
  "version": 2,
  "name": "recommendations-as-a-service",
  "builds": [
    { "src": "api/index.go", "use": "@now/go" }
  ],
  "routes": [
    { "src": "/api/v1/greet", "dest": "api/greet.go" },
    { "src": "/api/v1/users", "dest": "api/users/index.go" }
  ],
  "env": {
    "FAUNADB_SECRET_KEY": "@faunadb-secret-key-weekend",
    "GIN_MODE": "release"
  }
}
